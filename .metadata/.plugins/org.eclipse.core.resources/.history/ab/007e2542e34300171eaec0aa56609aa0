/** 
 * 
 */
package environment.staticenv.sign

import environment.EnvObject

/** 
 * @author AG
 * 
 */
class Stop extends EnvObject implements SignSymbol {
	val signType : SignType
	
	def new(signType : SignType) {
		this.signType = signType
	}
	
	override getType : SignType {
		this.signType
	}
	
}
